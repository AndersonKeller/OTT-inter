{
  "alias": ["dalecampeon.now.sh", "rivermas.now.sh", "riverplate.now.sh"],
  "build": {
    "env": {
      "TENANT": "river"
    }
  },
  "github": {
    "enabled": false
  },
  "routes": [
    { "src": "/fLogin", "dest": "/?modal=login&socialProvider=facebook" },
    { "src": "/gLogin", "dest": "/?modal=login&socialProvider=google" },
    { "src": "/login", "dest": "/?modal=login" }
  ],
  "scope": "somosgad",
  "version": 2
}
