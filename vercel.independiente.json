{
  "name": "independiente-app",
  "alias": [
    "independiente-app.vercel.app"
  ],
  "scope": "somosgad",
  "version": 2,
  "env": {
    "TENANT": "independiente",
    "API_URL": "https://independiente.dowhile.com.br"
  },
  "build": {
    "env": {
      "TENANT": "independiente",
      "API_URL": "https://independiente.dowhile.com.br"
    }
  },
  "routes": [
    {
      "src": "/fLogin",
      "dest": "/?modal=login&socialProvider=facebook"
    },
    {
      "src": "/gLogin",
      "dest": "/?modal=login&socialProvider=google"
    },
    {
      "src": "/login",
      "dest": "/?modal=login"
    }
  ]
}
